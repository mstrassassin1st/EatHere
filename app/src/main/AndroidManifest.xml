<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="showcase.project.dsc.eathere">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_eathere_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_eathere_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        
        <activity
            android:name=".MainActivity"
            android:label="Home" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".MenuList" />
        <activity android:name=".RestaurantList" />
        <activity android:name=".OrderList" />
        <activity android:name=".Payment" />
        <activity android:name=".PaymentRecipt" />
    </application>

</manifest>